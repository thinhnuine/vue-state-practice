{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "jsx": "preserve",
    "allowJs": true,
    "noImplicitAny": false,
    "strictNullChecks": false
  },
  "references": [{ "path": "./tsconfig.node.json" }]
}
